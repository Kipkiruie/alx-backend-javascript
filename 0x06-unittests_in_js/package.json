{
    "name": "unittests_in_js",
    "version": "1.0.0",
    "description": "A project for learning the basics of unittests in Node.js.",
    "author": "Elisha Kipkirui <elishak551@gmail.com>",
    "private": true,
    "license": "MIT",
    "scripts": {
        "lint": "./node_modules/.bin/eslint",
        "lint-all": "./node_modules/.bin/eslint [0-9]*.js",
        "lint-fix-all": "./node_modules/.bin/eslint --fix [0-9]*.js",
        "test": "mocha --require @babel/register --exit",
        "dev": "nodemon --exec babel-node --presets @babel/preset-env ./full_server/server.js ./database.csv"
    },
    "dependencies": {
        "chai-http": "^4.3.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/node": "^7.25.0",
        "@babel/preset-env": "^7.25.4",
        "@babel/register": "^7.24.6",
        "@eslint/js": "^9.9.0",
        "chai": "^4.2.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^28.8.0",
        "globals": "^15.9.0",
        "mocha": "^10.7.3",
        "nodemon": "^2.0.2",
        "request": "^2.88.0",
        "sinon": "^7.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Kipkiruie/alx-backend-javascript"
    },
    "bugs": {
        "url": "https://github.com/Kipkiruie/alx-backend-javascript/issues"
    },
    "homepage": "https://github.com/Kipkiruie/alx-backend-javascript/blob/main/0x06-unittests_in_js/README.md",
    "engines": {
        "node": "16.x",
        "npm": "8.x",
        "yarn": "1.x"
    }
}
